class Solution {
  public:
    vector<int> maxOfSubarrays(vector<int>& arr, int k) {
    int n=arr.size();
    vector<int> output;
    priority_queue<pair<int,int>> heap;
    for(int i=0;i<n;i++){
      heap.push({arr[i],i});
      
      if(i >= k-1){
          while(heap.top().second <= i-k){
              heap.pop();
          }
          output.push_back(heap.top().first);
      }
    }
      return output;
    }
};
